<entry key="script" type="xstring" value="cleanDF &lt;- knime.in%%00010################%%00010#compute overall NPS%%00010################%%00010totalSurveys &lt;- nrow(cleanDF)%%00010%%00010promotors &lt;- cleanDF$Likelihood_Recommend_H &gt; 8%%00010numPromotors &lt;- sum(promotors)%%00010%%00010detractors &lt;- cleanDF$Likelihood_Recommend_H &lt; 7%%00010numDetractors &lt;- sum(detractors)%%00010%%00010percentPromotors &lt;- numPromotors/totalSurveys%%00010percentDetractors &lt;- numDetractors/totalSurveys%%00010percentPassives &lt;- 1 - percentPromotors - percentDetractors%%00010NPS &lt;- percentPromotors - percentDetractors%%00010print(&quot;NPS&quot;)%%00010NPS%%00010%%00010###########################%%00010#Show NPS as a simple barchart%%00010###########################%%00010install.packages('ggplot2')%%00010library('ggplot2')%%00010NPS.vec &lt;- c(percentPromotors,percentPassives,percentDetractors)%%00010NPS.names &lt;- c(&quot;Promoters&quot;, &quot;Passives&quot;, &quot;Detractors&quot;)%%00010dfNPS &lt;- data.frame(NPS.names, NPS.vec)%%00010title &lt;- paste(&quot;Overall NPS &quot;, round(NPS, 2))%%00010ggplot(dfNPS, aes(x = NPS.names, y=NPS.vec)) + geom_col() + ggtitle(title) +  xlab(&quot;Category&quot;) + ylab(&quot;Percent&quot;)%%00010%%00010"/>
